# vim
alias e=$EDITOR

# git
alias ga="git add"
alias gb="git br"
alias gc="git ci"
alias gco="git co"
alias gd="git diff"
alias gs="git stash save --include-untracked"
alias gsp="git stash pop"
alias gst="git st"

# Unix
alias mkdir="mkdir -p"
alias ll="ls -al"

# Include custom aliases
if [[ -f ~/.aliases.local ]]; then
  source ~/.aliases.local
fi
